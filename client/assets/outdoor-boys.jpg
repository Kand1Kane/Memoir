<style>
	.post-tags {
		margin: 45px 0;
	}

	.post-tags h3 {
		margin-bottom: 30px;
	}

	.post-tags .tags-links a {
		padding: 10px 20px;
		background: #66C279;
		color: white;
		border-radius: 5px;
	}

	.expert-image {
		border: 3px solid #66c279;
	}

	.expert-user {
		font-size: 16px !important;
	}

	.post-content-container {
		max-width: calc(100% - 20px);
	}

	.more-stories img.post-thumbnail {
		width: 100%;
		height: auto;
	}

	.post-tags .tags-links a {
		margin-bottom: 8px;
		display: inline-block;
	}

	@media only screen and (max-width: 767px) {
		.post-content-container {
			max-width: 100%;
		}

		.expert-posts-container {
			margin-top: 10px;
			margin-bottom: 10px;
		}

		section.related-posts .container .row.post-grid .col-md-3.post {
			height: 100%;
		}
	}

	@media only screen and (min-width: 0px) {
		.single-post .single-post section.post .container .w-100 {
			width: 100% !important;
		}
	}
</style>

